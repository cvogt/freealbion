<?xml version="1.0" encoding="UTF-8"?>
<a2e_shader_include version="1">
	<vertex_shader>
		<variables>
attribute vec2 texture_coord;
varying vec2 tex_coord;
		</variables>
		<program>
void main() {
	gl_Position = gl_Vertex;
	tex_coord = texture_coord;
}
		</program>
	</vertex_shader>
</a2e_shader_include>