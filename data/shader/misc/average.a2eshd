<?xml version="1.0" encoding="UTF-8"?>
<a2e_shader version="1">
	<includes>hdr_shrink</includes>
	
	<fragment_shader>
		<variables>
uniform sampler2D rt_tex;

varying vec2 tex_coord;
		</variables>
		<program>
void main() {
	gl_FragColor = texture2D(rt_tex, tex_coord);
}
		</program>
	</fragment_shader>

</a2e_shader>